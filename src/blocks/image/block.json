{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "inseri-core/image",
	"title": "Image Box",
	"category": "inseri",
	"description": "Display image using the data of another block.",
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"blockName": {
			"type": "string",
			"default": ""
		},

		"input": {
			"type": "object",
			"default": {
				"key": "",
				"contentType": "",
				"description": ""
			}
		},
		"caption": {
			"type": "string",
			"default": ""
		},
		"altText": {
			"type": "string",
			"default": ""
		},
		"height": {
			"type": "integer",
			"default": null
		},
		"fit": {
			"type": "string",
			"default": "contain"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "inseri-core",
	"editorScript": "file:./index.js",
	"viewScript": ["file:./hydration.js"],
	"style": ["file:./index.css"],
	"script": "inseri-core"
}
