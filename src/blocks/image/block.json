{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "inseri-core/image",
	"title": "Image Box",
	"category": "inseri",
	"description": "Display image using the data of another block.",
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"inputKey": {
			"type": "string",
			"default": ""
		},
		"caption": {
			"type": "string",
			"default": ""
		},
		"altText": {
			"type": "string",
			"default": ""
		},
		"height": {
			"type": "integer",
			"default": null
		},
		"fit": {
			"type": "string",
			"default": "contain"
		},
		"metadata": {
			"type": "object",
			"default": {
				"name": ""
			}
		}
	},
	"supports": {
		"html": false,
		"align": true,
		"alignWide": true
	},
	"textdomain": "inseri-core",
	"editorScript": ["file:./index.js", "inseri-core-editor"],
	"viewScript": ["file:./hydration.js"],
	"style": ["file:./index.css"],
	"script": "inseri-core"
}
