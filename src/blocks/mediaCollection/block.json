{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "inseri-core/media-collection",
	"version": "0.1.0",
	"title": "Media Collection",
	"category": "inseri",
	"description": "Load multiple files from media library",
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"blockName": {
			"type": "string",
			"default": ""
		},
		"label": {
			"type": "string",
			"default": "Choose a file"
		},
		"output": {
			"type": "object",
			"default": {
				"key": "",
				"contentType": ""
			}
		},
		"fileIds": {
			"type": "array",
			"default": []
		},
		"selectedFileId": {
			"type": "string",
			"default": null
		},
		"isVisible": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "inseri-core",
	"editorScript": "file:./index.js",
	"viewScript": ["file:./hydration.js"],
	"style": ["file:./index.css"],
	"script": "inseri-core"
}
