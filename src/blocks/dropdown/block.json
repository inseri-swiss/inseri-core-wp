{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "inseri-core/dropdown",
	"title": "Dropdown",
	"category": "inseri",
	"description": "Provide options to choose from",
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"input": {
			"type": "object"
		},
		"output": {
			"type": "object"
		},
		"blockName": {
			"type": "string",
			"default": ""
		},
		"label": {
			"type": "string",
			"default": "Choose an item"
		},
		"searchable": {
			"type": "boolean",
			"default": true
		},
		"clearable": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "inseri-core",
	"editorScript": "file:./index.js",
	"viewScript": ["file:./hydration.js"],
	"script": "inseri-core"
}
