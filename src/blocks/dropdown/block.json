{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "inseri-core/dropdown",
	"title": "Dropdown",
	"category": "inseri",
	"description": "Provide guests options to choose from and the selected option can be reused in other blocks.",
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"inputKey": {
			"type": "string",
			"default": ""
		},
		"label": {
			"type": "string",
			"default": "Choose an item"
		},
		"searchable": {
			"type": "boolean",
			"default": true
		},
		"clearable": {
			"type": "boolean",
			"default": true
		},
		"metadata": {
			"type": "object",
			"default": {
				"name": ""
			}
		}
	},
	"supports": {
		"html": false,
		"align": true,
		"alignWide": true
	},
	"textdomain": "inseri-core",
	"editorScript": ["file:./index.js", "inseri-core-editor"],
	"viewScript": ["file:./hydration.js"],
	"script": "inseri-core"
}
