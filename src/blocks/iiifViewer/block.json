{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "inseri-core/iiif-viewer",
	"title": "IIIF Viewer",
	"category": "inseri",
	"description": "Display IIIF content using the other block's data.",
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"blockName": {
			"type": "string",
			"default": ""
		},
		"inputKey": {
			"type": "string",
			"default": ""
		},
		"showTitle": {
			"type": "boolean",
			"default": true
		},
		"showBadge": {
			"type": "boolean",
			"default": true
		},
		"showInformationPanel": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "inseri-core",
	"editorScript": "file:./index.js",
	"viewScript": ["file:./hydration.js"],
	"script": "inseri-core"
}
