{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "inseri-core/file-drop",
	"title": "Local File Import",
	"category": "inseri",
	"description": "Let guests provide their own data for blocks.",
	"attributes": {
		"blockId": {
			"type": "string",
			"default": ""
		},
		"blockName": {
			"type": "string",
			"default": ""
		},
		"mainText": {
			"type": "string",
			"default": "Drag and drop a file or click here"
		},
		"subText": {
			"type": "string",
			"default": "provide your file to process..."
		},
		"accepts": {
			"type": "array",
			"default": []
		},
		"multiple": {
			"type": "boolean",
			"default": false
		},
		"height": {
			"type": "integer",
			"default": 150
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "inseri-core",
	"editorScript": ["file:./index.js", "inseri-core-lighthouse-editor"],
	"viewScript": ["file:./hydration.js"],
	"style": ["file:./index.css"],
	"script": "inseri-core-lighthouse"
}
